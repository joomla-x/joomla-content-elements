{
  "name": "nibra/content-elements",
  "description": "A collection of Content Elements used by Joomla! X (Pythagoras)",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Niels Braczek",
      "email": "nbraczek@bsds.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "Joomla\\Content\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Joomla\\Tests\\Content\\": "tests"
    }
  },
  "require": {
    "nibra/joomla-standards-contenttypes": "@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1"
  },
  "scripts": {
    "docs": [
      "build/generate-docs.php"
    ],
    "pull": [
      "composer update nibra/joomla-standards-contenttypes"
    ],
    "icons": [
      "build/generate-icons.sh"
    ]
  }
}
